{
  "name": "tfm-frontend",
  "type": "static",
  "framework": "react",
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "installCommand": "npm install",
  "environment": {
    "REACT_APP_API_URL": "https://backend-tfm.julio.coolify.hgccarlos.es",
    "REACT_APP_APPWRITE_ENDPOINT": "https://appwrite-tfm.julio.coolify.hgccarlos.es/v1",
    "REACT_APP_APPWRITE_PROJECT_ID": "683f418d003d466cfe2e",
    "REACT_APP_APPWRITE_DATABASE_ID": "683f418d003d466cfe2e",
    "REACT_APP_APPWRITE_COLLECTION_ID": "683f418d003d466cfe2e",
    "REACT_APP_APPWRITE_BUCKET_ID": "683f418d003d466cfe2e",
    "REACT_APP_APPWRITE_TEAM_ID": "683f418d003d466cfe2e"
  },
  "ports": {
    "80": "80"
  },
  "healthcheck": {
    "path": "/",
    "interval": 30,
    "timeout": 10,
    "retries": 3
  }
} 