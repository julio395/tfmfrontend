{
  "name": "tfm",
  "type": "web",
  "framework": "react",
  "buildCommand": "CI=false GENERATE_SOURCEMAP=false npm run build",
  "installCommand": "rm -rf node_modules package-lock.json && npm install --legacy-peer-deps --no-audit --no-fund",
  "startCommand": "serve -s build",
  "healthcheckPath": "/",
  "healthcheckTimeout": 300,
  "restartPolicyType": "on-failure",
  "restartPolicyMaxRetries": 10,
  "env": {
    "REACT_APP_APPWRITE_ENDPOINT": "https://cloud.appwrite.io/v1",
    "REACT_APP_APPWRITE_PROJECT_ID": "67c8c0c0c0c0c0c0c0c0c0c0",
    "REACT_APP_API_URL": "https://projectfm.julio.coolify.hgccarlos.es/api/tfm",
    "NODE_ENV": "production",
    "CI": "false",
    "GENERATE_SOURCEMAP": "false",
    "REACT_APP_PLATFORM": "web",
    "REACT_APP_HOST": "https://projectfm.julio.coolify.hgccarlos.es",
    "REACT_APP_APPWRITE_PLATFORM": "web",
    "REACT_APP_APPWRITE_HOST": "https://projectfm.julio.coolify.hgccarlos.es",
    "REACT_APP_APPWRITE_ALLOW_SELF_SIGNED": "true",
    "REACT_APP_APPWRITE_CORS_ENABLED": "true"
  },
  "headers": {
    "Access-Control-Allow-Origin": "*",
    "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
    "Access-Control-Allow-Headers": "Content-Type, Authorization"
  }
} 